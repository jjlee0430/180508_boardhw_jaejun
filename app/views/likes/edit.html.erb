<h1>글 수정하기</h1>
<form action="/likes/update/<%= @like.id %>" method="post">
  <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
  제목 : <input type="text" name="edit_title" value="<%= @like.title %>"><br />
  내용 : <textarea name="edit_content" rows="5" cols"100"><%= @like.content %></textarea><br /><br />
  <input type="submit" value="수정">
</form>
